{
    "name": "dma",
    "displayName": "dma",
    "description": "Syntax highlighting and snippets for DMA files in vscode",
    "version": "0.0.1",
    "publisher": "reasors",
    "engines": {
        "vscode": "^1.33.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "contributes": {
        "languages": [
            {
                "id": "dma",
                "aliases": [
                    "dma"
                ],
                "extensions": [
                    "tlc",
                    "tlf",
                    "tsk"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "dma",
                "scopeName": "source.dma",
                "path": "./syntaxes/dma.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "dma",
                "path": "./snippets/snippets.json"
            }
        ]
    }
}
